@model IEnumerable<DeepakMishraWebAppMVC.Models.DashboardModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Dashboard [License Details]</title>
    <style>
        table,th,td{border:1px solid;background-color:yellowgreen;color:maroon;}
        btnlogout{position:fixed;right:10px;top:5px;}
    </style>
</head>
<body style="background-image: url('../Multimedia/Image/banner5.png');background-repeat:no-repeat;background-size:100%;height:auto; ">
    <header style="border: 1px solid;background-image: url('../Multimedia/Image/banner4.jpg'); background-repeat:no-repeat;background-size:100%; height: 100px; width: 100%; position: fixed; top: 0px; left: 0px;">
        <div class="pull-right" style="float:right;">
         
            @Html.ActionLink("LogOut", "Loginpage", "Login")  
          
        </div>
    </header>
  <div class="container" style="padding-top:110px;">
      <p>
          @*@Html.ActionLink("Create New", "Create")*@
      </p>
      <center>
          <div class="col-lg-12 col-md-12 col-sm-12">
              <div class="text-center"><h1 style="color:maroon;"><u><b>M.M.C.Soft Systems | License Management System</b></u></h1></div>
          </div>

      </center>
     
      <div class="row">
          <strong>Welcome User,</strong><br />
          <b>Please see your product details below..</b><br />
      </div>
      <table class="table" style="border:1px solid;" >
          
          <tr>
              <th>
                 Salutation
              </th>
              <th>
                 Employer Name
              </th>
              <th>
                 Company Name
              </th>
              <th>
                  Registered Email
              </th>
              <th>
                 Registered Contact
              </th>
              <th>
                  Address
              </th>
              <th>
                 Company Type
              </th>
              <th>
                  License No
              </th>
              <th>
                  Product Name
              </th>
              <th>
                  Product Type
              </th>
              <th>
                  Validity From
              </th>
              <th>
                  Validity TO
              </th>
             
          </tr>

          @foreach (var item in Model)
          {
              <tr>
                  <td>
                      @Html.DisplayFor(modelItem => item.salute)
                  </td>
                  <td>
                      @Html.DisplayFor(modelItem => item.ename)
                  </td>
                  <td>
                      @Html.DisplayFor(modelItem => item.cname)
                  </td>
                  <td>
                      @Html.DisplayFor(modelItem => item.email)
                  </td>
                  <td>
                      @Html.DisplayFor(modelItem => item.phone)
                  </td>
                  <td>
                      @Html.DisplayFor(modelItem => item.caddress)
                  </td>
                  <td>
                      @Html.DisplayFor(modelItem => item.ctype)
                  </td>
                  <td>
                      @Html.DisplayFor(modelItem => item.licenseno)
                  </td>
                  <td>
                      @Html.DisplayFor(modelItem => item.prodname)
                  </td>
                  <td>
                      @Html.DisplayFor(modelItem => item.prodtype)
                  </td>
                  <td>
                      @Html.DisplayFor(modelItem => item.validfrom)
                  </td>
                  <td>
                      @Html.DisplayFor(modelItem => item.validto)
                  </td>
                  @*<td>
                        @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                        @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                    </td>*@
              </tr>
          }

      </table>
      
  </div>
    <footer style="border: 1px solid; height: 100px;  width: 100%; position: fixed; bottom: 0px; left: 0px;background-image: url('../Multimedia/Image/banner1.png'); background-size:100%;background-position:left; background-repeat:no-repeat;">
       
    </footer>
</body>
</html>
